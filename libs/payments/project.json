{
    "name": "payments",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "projectType": "library",
    "sourceRoot": "libs/payments/src",
    "prefix": "placeos",
    "targets": {
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/libs/payments"],
            "options": {
                "jestConfig": "libs/payments/jest.config.ts",
                "passWithNoTests": true
            }
        },
        "lint": {
            "executor": "@nx/linter:eslint",
            "options": {
                "lintFilePatterns": [
                    "libs/payments/**/*.ts",
                    "libs/payments/**/*.html"
                ]
            }
        }
    },
    "tags": []
}
