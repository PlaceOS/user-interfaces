{
  "name": "@placeos/template-uis",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:watch": "ng test --watch --no-watchman",
    "test:ci": "ng test --ci --runInBand --no-watchman",
    "lint": "ng lint",
    "e2e": "npx cypress run",
    "semantic-release": "semantic-release",
    "postinstall": "node ./config/version.js && ng build",
    "postversion": "node ./config/version.js"
  },
  "private": true,
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/test.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/cypress/",
      "<rootDir>/src/test.ts"
    ]
  },
  "dependencies": {
    "@angular/animations": "~10.0.2",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "~10.0.2",
    "@angular/compiler": "~10.0.2",
    "@angular/core": "~10.0.2",
    "@angular/forms": "~10.0.2",
    "@angular/localize": "^10.0.12",
    "@angular/material": "^10.1.3",
    "@angular/platform-browser": "~10.0.2",
    "@angular/platform-browser-dynamic": "~10.0.2",
    "@angular/router": "~10.0.2",
    "@angular/service-worker": "~10.0.2",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "^10.0.0",
    "@angular-devkit/build-angular": "~0.1000.1",
    "@angular-devkit/build-ng-packagr": "~0.1000.7",
    "@angular/cli": "~10.0.1",
    "@angular/compiler-cli": "~10.0.2",
    "@placeos/ts-client": "^3.2.1",
    "@types/jest": "^26.0.10",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0-next.1",
    "jest": "^26.4.2",
    "jest-preset-angular": "^8.3.1",
    "ng-mocks": "^10.1.2",
    "ng-packagr": "^10.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  }
}
